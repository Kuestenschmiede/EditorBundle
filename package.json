{
  "name": "editorbundle",
  "version": "1.0.0",
  "description": "con4gis-Editor ============ The interactive maps brick of the Contao GIS-kit **con4gis**.",
  "main": "webpack.config.js",
  "scripts": {
    "encore": "encore",
    "test": "jest --no-cache"
  },
  "jest": {
    "modulePaths": [
      "/Resources/public/js",
      "<rootDir>/../MapsBundle/Resources/public/vendor"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "Resources",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/Resources/public/js/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/Resources/public/js/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kuestenschmiede/EditorBundle.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kuestenschmiede/EditorBundle/issues"
  },
  "homepage": "https://github.com/Kuestenschmiede/EditorBundle#readme",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.22.2",
    "canvas": "^2.3.1",
    "jest": "^23.6.0",
    "jquery": "^3.3.1"
  },
  "dependencies": {
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "regenerator-runtime": "^0.13.1",
    "source-map-support": "^0.5.10"
  }
}
